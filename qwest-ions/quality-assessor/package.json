{
  "name": "@learnqwest/quality-assessor",
  "version": "1.0.0",
  "description": "Quality Assessment Qwest-ion - Educational content quality filtering with 5-dimension scoring",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "dev": "bun run src/index.ts",
    "build": "bun build src/index.ts --outfile=dist/quality-assessor --target=bun",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts tests/**/*.ts"
  },
  "dependencies": {
    "zod": "^3.22.4",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/bun": "^1.0.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "learnqwest",
    "quality-assessment",
    "educational-content",
    "content-filtering",
    "qwest-ion"
  ],
  "author": "LearnQwest",
  "license": "MIT"
}
